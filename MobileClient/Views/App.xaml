<?xml version="1.0" encoding="UTF-8" ?>
<Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="HealthProject.Views.App"
             xmlns:local="clr-namespace:HealthProject.Converters">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Resources/Styles/Colors.xaml" />
                <ResourceDictionary Source="/Resources/Styles/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <local:BoolToColorConverter x:Key="BoolToColorConverter" />
            <local:IsCollectionEmptyConverter x:Key="IsCollectionEmptyConverter" />
            <local:SelectedItemToTextColorConverter x:Key="SelectedItemToTextColorConverter" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
